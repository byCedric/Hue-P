{
  "expo": {
    "name": "hue-p",
    "slug": "hue-p",
    "owner": "bycedric",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app-icon.png",
    "splash": {
      "image": "./src/assets/images/app-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#222B45"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "src/assets/**/*",
      "react-native-vector-icons/Fonts/MaterialIcons.ttf",
      "@expo-google-fonts/raleway/Raleway_700Bold.ttf",
      "@expo-google-fonts/open-sans/OpenSans_400Regular.ttf",
      "@expo-google-fonts/open-sans/OpenSans_600SemiBold.ttf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bycedric.huep"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app-icon-adaptive.png",
        "backgroundColor": "#222B45"
      },
      "package": "com.bycedric.huep",
      "permissions": [
        "INTERNET"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "extra": {
      "eas": {
        "projectId": "c4791c75-f6de-4aa3-8408-67626d422b43"
      }
    }
  }
}
