{
	"expo": {
		"name": "hue-p",
		"slug": "hue-p",
		"owner": "bycedric",
		"platforms": [
			"ios",
			"android",
			"web"
		],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/images/app-icon.png",
		"splash": {
			"image": "./src/assets/images/app-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#222B45"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"src/assets/**/*"
		],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.bycedric.huep"
		},
		"android": {
			"package": "com.bycedric.huep",
			"permissions": []
		},
		"androidStatusBar": {
			"barStyle": "light-content"
		}
	}
}
